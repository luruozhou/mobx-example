{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "outDir": "dist",
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "watch": true,
    "types": ["webpack-env"],
    "lib": ["es2017", "dom"]
  },
  "exclude": ["dist", "node_modules"],
  "include": ["./src/**/*.ts", "./src/**/*.tsx"]
}
